<?xml version="1.0" encoding="UTF-8"?>
<SearchPlugin xmlns:os="http://a9.com/-/spec/opensearch/1.1/" xmlns="http://www.mozilla.org/2006/browser/search/">
<os:ShortName>Splunk</os:ShortName>
<os:Description>Search Splunk from your search bar!</os:Description>
<os:InputEncoding>UTF-8</os:InputEncoding>
<os:Image width="16" height="16">data:image/x-icon;base64,</os:Image>
<os:Url type="text/html" method="GET" template="http{% if https %}s{% endif %}://{{ host }}{% if port %}:{{ port }}{% endif %}/app/search/search?q={% raw %}{searchTerms}{% endraw %}" resultDomain="{{ host }">
</os:Url>
</SearchPlugin>
